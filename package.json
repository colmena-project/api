{
  "name": "colmena-core-api",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon --ignore '**/logs/.*' --watch 'src/**/*' -e ts,js,json --exec 'ts-node src/server.ts'",
    "inspect": "nodemon --ignore '**/logs/.*' --watch 'src/**/*' -e ts,js,json --exec 'node --inspect=0.0.0.0 -r ts-node/register src/server.ts'",
    "start": "node ./dist/server.js",
    "eslint": "eslint --debug --ext .ts src/cloud",
    "seed": "for filename in ./seeds/*; do mongoimport --mode=upsert --uri ${MONGO_DSN} --file $filename;  done",
    "exportCollection": "dump() { mongoexport --uri ${MONGO_DSN} --collection $1 -o \"./seeds/$1.json\"  ; }; dump",
    "exportDB": "mongodump --uri ${MONGO_DSN} -o './backups/'$( date -Iseconds)",
    "importDB": "mongorestore --uri ${MONGO_DSN}",
    "madge-circular": "madge --circular ./src/cloud/main.ts",
    "madge-orphans": "madge --orphans ./src/cloud/main.js",
    "build": "npm run madge-circular && tsc",
    "tsc": "tsc",
    "prepublish": "npm run build"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^2.3.1",
    "@sendgrid/mail": "^6.5.3",
    "escape-html": "^1.0.3",
    "expo-server-sdk": "^3.1.0",
    "express": "^4.17.1",
    "hat": "0.0.3",
    "parse": "^2.12.0",
    "parse-dashboard": "^2.0.5",
    "parse-server": "^4.2.0",
    "saslprep": "^1.0.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/node": "^13.11.1",
    "@types/parse": "^2.10.18",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "madge": "^3.8.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  }
}
